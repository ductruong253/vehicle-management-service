{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "group",
  "type": "object",
  "properties": {
    "make": {
      "type": "string",
      "minLength": 2,
      "pattern": "^[^-\\s][a-zA-Z0-9_\\s-]+$"
    },
    "model": {
      "type": "string",
      "minLength": 2,
      "pattern": "^[^-\\s][a-zA-Z0-9_\\s-]+$"
    },
    "year": {
      "type": "string",
      "minLength": 4,
      "pattern": "^[^-\\s][0-9]{3}$"
    },
    "color": {
      "type": "string",
      "minLength": 2,
      "pattern": "^[^-\\s][a-zA-Z0-9_\\s-]+$"
    }
  },
  "required": [
    "make",
    "vehicleModel",
    "year",
    "color"
  ],
  "additionalProperties": false
}
